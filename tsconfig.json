      "@setups": [ "./__tests__/setups" ],
      "@shared": [ "src/presentation/modules/shared" ],
      "@shared/*": [ "src/presentation/modules/shared/*" ],
      "@test-config": [ "./__tests__/config" ],
      "@test-setup": [ "./jest.setup.ts" ],
      "@theme": [ "src/presentation/modules/theme" ],
      "@theme/*": [ "src/presentation/modules/theme/*" ],
      "@ui": [ "src/presentation/modules/ui" ],
      "@ui/*": [ "src/presentation/modules/ui/*" ],
      "@utils": [ "src/presentation/utils" ],
      "@utils/*": [ "src/presentation/utils/*" ],
    }    
  }
}
